\n}\n\nbody {\n  max-width: 1920px;\n  min-height: 100vh;\n  display: grid;\n  place-items: center;\n  padding: 1.5rem;\n  font-family: var(--font-family);\n  background: #e2e8ff;\n}\n\n.orb-canvas {\n  position: fixed;\n  top: 0;\n  left: 0;\n  width: 100%;\n  height: 100%;\n  pointer-events: none;\n  z-index: -1;\n}\n\nstrong {\n  font-weight: 600;\n}\n\n.overlay {\n  position: relative;\n  width: 100%;\n  height: 100%;\n  padding: 8rem 10rem;\n  display: flex;\n  align-items: center;\n  justify-content: center;\n  background: white;\n  background-image:url(\"https://i.imgur.com/Z45zcHU.png\");\n  background-size: contain;\n  box-shadow: 0 0.75rem 2rem 0 rgba(0, 0, 0, 0.1);\n  border-radius: 2rem;\n  border: 1px solid rgba(255, 255, 255, 0.125);\n}\n\n.overlay__inner {\n  text-align:center;\n}\n\n.navbar{\n  width: 100%;\n  height: 65px;\n  position: absolute;\n  top: 0;\n  left:0;\n}\n\n.footer{\n  display: flex;\n  justify-content: space-between;\n  align-items: center;\n  height: 65px;\n  position: absolute;\n  bottom: 0;\n  left:10px;\n  right:10px;\n  background: rgba(226, 232, 255, 0.5);\n  border-radius: 2rem;\n  margin-bottom:10px;\n  padding-left:9.5rem;\n  padding-right:9.5rem;\n}\n\n.footer-links-left {\n  display: flex;\n  list-style: none;\n  margin: 0;\n  padding: 0;\n}\n\n.footer-links-left li {\n  margin-right: 40px;\n}\n\n.footer-links-left li:last-child {\n  margin-right: 0;\n}\n\n.footer-links-left li a {\n  text-decoration: none;\n  color: #333;\n}\n\n.logo {\n  height: 35px;\n  position: absolute;\n  top: 10px;\n  left: 30px;\n}\n\n.connect_wallet {\n  width: 150px;\n  padding: 14px;\n  border-radius: 20px;\n  border: 0;\n  background-color: rgb(253, 234, 241);\n  color: rgb(213, 0, 102);\n  font-weight: bold;\n  position: absolute;\n  top: 10px;\n  right: 30px;\n}\n\n.connect_wallet:hover {\n  color: rgb(253, 90, 152);\n  cursor: pointer;\n}\n\n.overlay__title {\n  font-size: 3.5rem;\n  line-height: 3.75rem;\n  font-weight: 800;\n  margin-bottom: 2rem;\n}\n.overlay__title span{\n  color:rgb(213, 0, 102);\n}\n.overlay__description {\n  font-size: 0.8rem;\n  line-height: 1.75rem;\n  margin-bottom: 3rem;\n}\n\n.overlay__btns {\n  width: 100%;\n  max-width: 30rem;\n  display: flex;\n  margin: 0 auto;\n}\n\n.overlay__btn {\n  width: 50%;\n  height: 2.5rem;\n  display: flex;\n  justify-content: center;\n  align-items: center;\n  font-size: 0.875rem;\n  font-weight: 600;\n  color: white;\n  background-color: rgb(6, 0, 24);\n  border: none;\n  border-radius: 0.5rem;\n  transition: transform 150ms ease;\n  outline-color: hsl(var(--hue), 95%, 50%);\n}\n\n.overlay__btn:hover {\n  transform: scale(1.05);\n  cursor: pointer;\n}\n\n.overlay__btn--transparent {\n  background: transparent;\n  color: var(--dark-color);\n  border: 2px rgb(6, 0, 24) solid;\n  border-width: 2px;\n  margin-right: 0.75rem;\n}\n\n.overlay__btn-emoji {\n  margin-left: 0.375rem;\n}\n\na {\n  text-decoration: none;\n  color: var(--dark-color);\n  width: 100%;\n  height: 100%;\n  display: flex;\n  justify-content: center;\n  align-items: center;\n}\n\n/* Not too many browser support this yet but it's good to add! */\n@media (prefers-contrast: high) {\n  .orb-canvas {\n    display: none;\n  }\n}\n\n@media only screen and (max-width: 1140px) {\n  .overlay {\n    padding: 8rem 4rem;\n  }\n}\n\n@media only screen and (max-width: 840px) {\n  body {\n    padding: 1.5rem;\n  }\n\n  .overlay {\n    padding: 4rem;\n    height: auto;\n  }\n\n  .overlay__title {\n    font-size: 1.25rem;\n    line-height: 2rem;\n    margin-bottom: 1.5rem;\n  }\n\n  .overlay__description {\n    font-size: 0.875rem;\n    line-height: 1.5rem;\n    margin-bottom: 2.5rem;\n  }\n}\n\n@media only screen and (max-width: 600px) {\n  .overlay {\n    padding: 1.5rem;\n  }\n\n  .overlay__btns {\n    flex-wrap: wrap;\n  }\n\n  .overlay__btn {\n    width: 100%;\n    font-size: 0.75rem;\n    margin-right: 0;\n  }\n\n  .overlay__btn:first-child {\n    margin-bottom: 1rem;\n  }\n}\n"},"lineC